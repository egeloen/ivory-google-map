<?xml version="1.0" ?>

<serializer
    xmlns="http://egeloen.fr/schema/ivory-serializer"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://egeloen.fr/schema/ivory-serializer
                        http://egeloen.fr/schema/ivory-serializer/mapping-1.0.xsd"
>
    <class
        name="Ivory\GoogleMap\Service\TimeZone\Response\TimeZoneResponse"
        exclusion-policy="all"
        xml-root="TimeZoneResponse"
    >
        <property
            name="status"
            type="string"
            expose="true"
        />

        <property
            name="dstOffset"
            type="integer"
            expose="true"
        />

        <property
            name="rawOffset"
            type="integer"
            expose="true"
        />

        <property
            name="timeZoneId"
            type="string"
            expose="true"
        />

        <property
            name="timeZoneName"
            type="string"
            expose="true"
        />
    </class>
</serializer>
