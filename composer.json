{
    "name": "validaide/google-map",
    "description": "Google Map API v3 integration - PHP >= 7.0",
    "keywords": [
        "google",
        "map",
        "validaide"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        },
        {
            "name": "Mark Bijl",
            "email": "mark.bijl@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.4.13"
        }
    },
    "require": {
        "php": "^7.4",
        "egeloen/json-builder": "^3.0",
        "symfony/event-dispatcher": "3.4.*",
        "ext-json": "*"
    },
    "require-dev": {
        "egeloen/serializer": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "php-http/cache-plugin": "^1.4",
        "php-http/guzzle6-adapter": "^1.0",
        "phpunit/phpunit": "7.*",
        "phpunit/phpunit-selenium": "7.*",
        "symfony/cache": "3.4.*",
        "symfony/phpunit-bridge": "3.4.*"
    },
    "suggest": {
        "egeloen/serializer": "Allows to use http services",
        "php-http/client-implementation": "Allows to use http services",
        "php-http/message": "Allows to use http services"
    },
    "autoload": {
        "psr-4": {
            "Ivory\\GoogleMap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ivory\\Tests\\GoogleMap\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
